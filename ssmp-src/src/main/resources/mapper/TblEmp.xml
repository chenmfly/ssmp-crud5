<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.crud.dao.TblEmpMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.atguigu.crud.bean.TblEmp">
        <id column="emp_id" property="empId" />
        <result column="emp_name" property="empName" />
        <result column="gender" property="gender" />
        <result column="email" property="email" />
        <result column="d_id" property="dId" />
        <result column="rm_flag" property="rmFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        emp_id AS empId, emp_name AS empName, gender, email, d_id AS dId, rm_flag AS rmFlag
    </sql>

</mapper>
